CHIP RAM4K {
    IN in[16], load, address[12];
    OUT out[16];

    PARTS:
      DMux8Way(
        in=load,
        sel=address[9..11],
        a=dmux8wayOut1,
        b=dmux8wayOut2,
        c=dmux8wayOut3,
        d=dmux8wayOut4,
        e=dmux8wayOut5,
        f=dmux8wayOut6,
        g=dmux8wayOut7,
        h=dmux8wayOut8
      );

      RAM512(in=in, load=dmux8wayOut1, address=address[0..8], out=registerOut1);
      RAM512(in=in, load=dmux8wayOut2, address=address[0..8], out=registerOut2);
      RAM512(in=in, load=dmux8wayOut3, address=address[0..8], out=registerOut3);
      RAM512(in=in, load=dmux8wayOut4, address=address[0..8], out=registerOut4);
      RAM512(in=in, load=dmux8wayOut5, address=address[0..8], out=registerOut5);
      RAM512(in=in, load=dmux8wayOut6, address=address[0..8], out=registerOut6);
      RAM512(in=in, load=dmux8wayOut7, address=address[0..8], out=registerOut7);
      RAM512(in=in, load=dmux8wayOut8, address=address[0..8], out=registerOut8);

      Mux8Way16(
        a=registerOut1,
        b=registerOut2,
        c=registerOut3,
        d=registerOut4,
        e=registerOut5,
        f=registerOut6,
        g=registerOut7,
        h=registerOut8,
        sel=address[9..11],
        out=out
      );
}
